#To transform Julia array to R array
array_to_R_format = function(array){
return(gsub("\\]",")",gsub("\\[","c(",array)))
}
calculate_pt_z_K = function(data, summary_var){
data=data[gen==last(gen)]
data[,K_rounded := f_round2(K,0.5)]
data[,max_K_k_rounded := f_round2(max_K_k,0.5)]
data[,mean_K_k_rounded := f_round2(mean_K_k,0.5)]
if(missing(summary_var)){
data[,mean_z_by_K:=mean(z),by=K_rounded]
data[,mean_z_by_maxK:=mean(z),by=max_K_k_rounded]
data[,mean_z_by_meanK:=mean(z),by=mean_K_k_rounded]
data[,se_z_by_K:=se(z),by=K_rounded]
data[,se_z_by_maxK:=se(z),by=max_K_k_rounded]
data[,se_z_by_meanK:=se(z),by=mean_K_k_rounded]
pt_z_K=ggplot(data)+
geom_ribbon(aes(x=K_rounded,ymax=mean_z_by_K+se_z_by_K,ymin=mean_z_by_K - se_z_by_K),fill="grey50")+
geom_point(aes(x=K_rounded,y=mean_z_by_K,colour="K"))+
geom_line(aes(x=K_rounded,y=mean_z_by_K))+
geom_ribbon(aes(x=max_K_k_rounded,ymax=mean_z_by_maxK+se_z_by_maxK,ymin=mean_z_by_maxK - se_z_by_maxK),fill="lightblue")+
geom_point(aes(x=max_K_k_rounded,y=mean_z_by_maxK,colour="maxK"))+
geom_line(aes(x=max_K_k_rounded,y=mean_z_by_maxK,colour="maxK"))+
geom_ribbon(aes(x=mean_K_k_rounded,ymax=mean_z_by_meanK+se_z_by_meanK,ymin=mean_z_by_meanK - se_z_by_meanK),fill="coral")+
geom_point(aes(x=mean_K_k_rounded,y=mean_z_by_meanK,colour="meanK"))+
geom_line(aes(x=mean_K_k_rounded,y=mean_z_by_meanK,colour="meanK"))+
u_theme1+
ylim(0,1)+
ylab("Average level of inequality")+
xlab("Productivity")+
scale_color_manual(name="Inequality\n as a function\n of productivity of",values = c("K" = "black","maxK" = "blue","meanK" = "red"),labels=c("current patches","maximum neighbouring\n patches","mean neighbours\n patches"))
}else{
data[,mean_z_by_K:=mean(z),by=c(summary_var,"K_rounded")]
data[,mean_z_by_maxK:=mean(z),by=c(summary_var,"max_K_k_rounded")]
data[,mean_z_by_meanK:=mean(z),by=c(summary_var,"mean_K_k_rounded")]
data[,se_z_by_K:=se(z),by=c(summary_var,"K_rounded")]
data[,se_z_by_maxK:=se(z),by=c(summary_var,"max_K_k_rounded")]
data[,se_z_by_meanK:=se(z),by=c(summary_var,"mean_K_k_rounded")]
pt_z_K=ggplot(data)+
geom_ribbon(aes(x=K_rounded,ymax=mean_z_by_K+se_z_by_K,ymin=mean_z_by_K - se_z_by_K),fill="grey50")+
geom_point(aes(x=K_rounded,y=mean_z_by_K,colour="K"))+
geom_line(aes(x=K_rounded,y=mean_z_by_K))+
geom_ribbon(aes(x=max_K_k_rounded,ymax=mean_z_by_maxK+se_z_by_maxK,ymin=mean_z_by_maxK - se_z_by_maxK),fill="lightblue")+
geom_point(aes(x=max_K_k_rounded,y=mean_z_by_maxK,colour="maxK"))+
geom_line(aes(x=max_K_k_rounded,y=mean_z_by_maxK,colour="maxK"))+
geom_ribbon(aes(x=mean_K_k_rounded,ymax=mean_z_by_meanK+se_z_by_meanK,ymin=mean_z_by_meanK - se_z_by_meanK),fill="coral")+
geom_point(aes(x=mean_K_k_rounded,y=mean_z_by_meanK,colour="meanK"))+
geom_line(aes(x=mean_K_k_rounded,y=mean_z_by_meanK,colour="meanK"))+
u_theme1+
ylim(0,1)+
ylab("Average level of inequality")+
xlab("Productivity")+
facet_grid(as.formula(paste(".~", summary_var)))+
scale_color_manual(name="Inequality\n as a function of\n productivity of",values = c("K" = "black","maxK" = "blue","meanK" = "red"),labels=c("current patches","maximum\nneighbours","mean\nneighbours"))
}
return(pt_z_K)
}
